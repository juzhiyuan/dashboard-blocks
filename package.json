{
  "private": true,
  "scripts": {
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged && node ./.script/genBlockConfig.js && git add ./umi-block.json",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./ && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "screenshot": "pro screenshot",
    "start": "umi dev",
    "test": "umi test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-cli": "^2.0.2",
    "@ant-design/pro-layout": "6.0.0-2",
    "antd": "^4.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "umi": "^3.1.0",
    "umi-request": "^1.0.8"
  },
  "devDependencies": {
    "@types/react-dom": "^16.8.4",
    "@umijs/fabric": "^2.0.5",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-react": "^1.4.24",
    "@umijs/plugin-block-devtool": "^1.1.1",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "husky": "^4.0.7",
    "lint-staged": "^10.0.1",
    "prettier": "^2.0.2",
    "stylelint": "^13.0.0",
    "typescript": "^3.5.1"
  },
  "checkFiles": [
    "**/*.ts*",
    "**/*.less",
    "**/*.md"
  ]
}
